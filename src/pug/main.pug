    .team__members.row.mt30
      -
        var teamNumber = 4, i = 0,
        imgPath = './imgs/',
        base = 'team_0',
        photoCaptions = {};
        photoCaptions['1'] = {};
        photoCaptions['1']['title'] = 'John Doe';
        photoCaptions['1']['text'] = 'Founder / CEO';
        photoCaptions['2'] = {};
        photoCaptions['2']['title'] = 'Mike Doe';
        photoCaptions['2']['text'] = 'Web Designer';
        photoCaptions['3'] = {};
        photoCaptions['3']['title'] = 'Jane Doe';
        photoCaptions['3']['text'] = 'Creative Director';
        photoCaptions['4'] = {};
        photoCaptions['4']['title'] = 'Larry Show';
        photoCaptions['4']['text'] = 'Project Manager';
        
      while i++ < teamNumber
        - var imgSrc = imgPath + base + i + '.jpg';
        figure.figure
          img(src=imgSrc, class="photo", width="225", height="225", alt="member photo")
          figcaption.figcaption.text-center
            .theme-canvas__title.figcaption__title=photoCaptions[i]['title']
            .figcaption__text=photoCaptions[i]['text']
  
