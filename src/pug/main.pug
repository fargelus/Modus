  section.portfolio.theme-canvas#portfolio
    h2.theme-canvas__title Our portfolio
    p(class="text-center").mt30.
      Lorem ipsum dolor sit amet, consectetur adipiscing elit. 
      Duis sed dapibus leo nec ornare diamcommodo nibh ante facilisis.
    
    .cards
      .tabs.cards__tabs
        -
          var tabLinkNames = ['All', 'Web Design',
          'Web Development', 'Branding'];
        each val in tabLinkNames
          button.button.button--tab.tabs__button.js-button--tab=val
      
      .cards-deck.row
        -
          var n = 0, imageNumber = 8, base = 'portfolio_0';
        while ++n < (imageNumber + 1)
          - var imgSrc = './imgs/' + base + n + '.jpg';
          .cards-deck__item
            img(src=imgSrc, width="600", height="420", alt="some stuff")
            
            .popup-block.bg-theme-dark.theme-image
              h4.theme-image__title.popup-block__title Project title
              -
                var textValue = '';
                switch (n) {
                  case 1:
                  case 3:
                  case 4:
                  case 5:
                    textValue = 'Web Design';
                    break;
                  case 2:
                  case 6:
                    textValue = 'Web Development';
                    break;
                  case 7:
                    textValue = 'Web Development, Branding';
                    break;
                  case 8:
                    textValue = 'Branding';
                    break;
                  default:
                    break;
                }
              
              p.popup-block__text.js-tab-anchor=textValue
              
  section.team.theme-canvas.bg-theme-gainsboro#team
