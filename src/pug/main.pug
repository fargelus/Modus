    .contact-info__feedback.feedback
      h3.theme-canvas__title.mt30.text-center.feedback__title Leave us a message
      form(action="some.php", method="post", name="contact_form").form
        .row.form__row
          input(type="text", placeholder="Name").form__elem
          input(type="email", placeholder="Email").form__elem
        textarea(placeholder="Message", rows="5").form__elem
        
        button(class="button form__submit-button button--small", type="submit") Send Message
    
    .contact-info__socials
      h3.theme-canvas__title.text-center Follow us
      ul
        -
          var classToAddressMap = {
            'fa-facebook': 'https://ru-ru.facebook.com/',
            'fa-twitter': 'https://twitter.com/',
            'fa-dribbble': 'https://dribbble.com/',
            'fa-github': 'https://github.com/',
            'fa-instagram': 'https://www.instagram.com/',
            'fa-linkedin': 'https://linkedin.com'
          };
      
        each address, className in classToAddressMap
          - var fullClassName = 'icon fa ' + className;
          li.list-item
            a(href=address, class="social-link")
              i(class=fullClassName)
              
